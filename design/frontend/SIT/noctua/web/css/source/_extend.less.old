/** Web-form help tool-tip designing[START][11-10-2019] */
.webforms .tool-tip {
    width: 20px;
    height: 20px;
    display: inline-block;
    background: #551805;
    padding: 0;
    margin: 0 5px;
    border-radius: 50%;
    cursor: pointer;
    position: absolute;
    top: 8px;
    right: -30px;
}
.webforms .tool-tip:after {
    content: '?';
    font-weight: 700;
    color: #fff;
    font-size: 12px;
    height: 15px;
    width: 10px;
    padding: 2px 7px;
    margin: 0;
    display: block;
}
.webforms .tooltip-content {
    background: #1c1c1c;
    color: #fff;
    width: 356px;
    position: absolute;
    right: -13%;
    text-align: justify;
    bottom: 32px;
    padding: 5px;
    border-radius: 5px;
    font-size: 12px;
    display: none;
}
.webforms .type-file .tooltip-content {
    bottom: 40px;
}
.webforms .tooltip-content:after {
    content: "";
    position: absolute;
    top: 100%;
    margin-left: -5px;
    border-width: 6px;
    border-style: solid;
    border-color: #1c1c1c transparent transparent;
    right: 30px;
}
.webforms .tool-tip:hover~.tooltip-content {
    display: block;
}
@media screen and (max-width: 1001px) {
    .webforms .tool-tip:hover+.tooltip-content {
        display: inline-block;
        height: fit-content;
        max-width: 356px;
        line-height: normal;
        top: -36px;
        font-size: 13.5px;
        position: absolute;
        right: -50px;
        z-index: 9;
        text-align: justify;
    }
}
@media screen and (max-width: 767px) {
    .webforms .fieldset,
    .webforms .webforms-description-text {
        padding: 0 !important;
        width: 81% !important;
        margin: 0px auto !important;
        float: none !important;
    }
}
@media screen and (max-width: 540px) {
    .webforms .tooltip-content:after {
        right: 5px;
    }
    .webforms .tool-tip:hover+.tooltip-content {
        top: -42px;
        right: -25px;
        width: 100%;
    }
}
