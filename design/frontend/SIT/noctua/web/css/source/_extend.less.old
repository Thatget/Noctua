/** Menu icon design  */

.block-search .label {
    position: relative;
    top: 2px;
    right: 20px;
}
.mobile-sticky-menu .block-search .label {
    top: 0px;
}
.mobile-sticky-menu .nav-toggle {
    top: 3px;
}
.nav-toggle {
    width: 27px;
    height: auto;
    position: relative;
    -webkit-transform: rotate(0deg);
    -moz-transform: rotate(0deg);
    -o-transform: rotate(0deg);
    transform: rotate(0deg);
    -webkit-transition: -webkit-transform 0.5s ease-in-out;
    -moz-transition: -moz-transform 0.5s ease-in-out;
    -o-transition: -o-transform 0.5s ease-in-out;
    transition: transform 0.5s ease-in-out;
    cursor: pointer;
    margin-right: 5px;
    top: 38px;
}

.nav-toggle .toggle-nav-icon {
    display: block;
    position: absolute;
    height: 3px;
    width: 100%;
    background: #F8F8F8;
    border-radius: 9px;
    opacity: 1;
    left: 0;
    -webkit-transform: rotate(0deg);
    -moz-transform: rotate(0deg);
    -o-transform: rotate(0deg);
    transform: rotate(0deg);
    -webkit-transition: all .25s ease-in-out;
    -moz-transition: all .25s ease-in-out;
    -o-transition: all .25s ease-in-out;
    transition: all .25s ease-in-out;
    clip: inherit;
}
.nav-toggle .toggle-nav-icon:nth-child(1) {
    top: 13px;
}

.nav-toggle .toggle-nav-icon:nth-child(2),.nav-toggle .toggle-nav-icon:nth-child(3) {
    top: 22px;
}

.nav-toggle .toggle-nav-icon:nth-child(4) {
    top: 31px;
}

.nav-toggle.mobile-menu .toggle-nav-icon:nth-child(1) {
    top: 18px;
    width: 0%;
    left: 50%;
}

.nav-toggle.mobile-menu .toggle-nav-icon:nth-child(2) {
    -webkit-transform: rotate(45deg);
    -moz-transform: rotate(45deg);
    -o-transform: rotate(45deg);
    transform: rotate(45deg);
}

.nav-toggle.mobile-menu .toggle-nav-icon:nth-child(3) {
    -webkit-transform: rotate(-45deg);
    -moz-transform: rotate(-45deg);
    -o-transform: rotate(-45deg);
    transform: rotate(-45deg);
}

.nav-toggle.mobile-menu .toggle-nav-icon:nth-child(4) {
    top: 18px;
    width: 0%;
    left: 50%;
}
/** Menu icon design end  */

