/*Added By MJ [For 360 view] [29.05.2019]*/
.sit-product-360-view-container .section-inner-presse-page {
    margin: 92px 0 9px 0px;
    width: 100%;
    padding: 0;
    text-align: justify;
}
.sit-product-360-view-container {
    width: 635px;
    float: right;
    margin-right: 65px;
    position: relative;
    font-size: 12px;
    margin-bottom: 45px;
}
.sit-product-360-view-container .sit-product-360-view-head{
  width: 100%;
}
.sit-product-360-view-container .sit-product-360-view-head h2 {
    margin: 0 0 3px;
    padding: 0;
    color: #451807;
    padding-bottom: 6px;
    text-transform: uppercase;
    border-bottom: 5px solid #eee;
    font-size: 20px;
    line-height: 18px;
    letter-spacing: 0.1px;
    font-weight: 400;
}
.sit-product-360-view-container .sit-product-360-view {
    margin: 5px 0 auto;
    display: block;
    position: relative;
    width: 100%;
    text-align: right;
}
.sit-product-360-view-textarea-container span {
    display: block;
    margin: 0;
}
.sit-product-360-view-textarea-container textarea.sit-product-360-textarea{
    min-height: 200px;
    padding: 2px;
    font-family: monospace;
    margin-bottom: 30px;
    font-size: 13px;
}
.sit-product-360-view-textarea-container textarea.sit-product-360-textarea:hover
+ .sit-360-copy-button input.copy-button{
    display: block;
}
.sit-360-copy-button input.copy-button:hover{
    display: block;
}
.sit-product-360-view-textarea-container .sit-360-textarea{
    position: relative;
}
.sit-360-textarea .sit-360-copy-button{
    position: absolute;
    top: 0;
    right: 0;
}
.sit-360-copy-button input.copy-button {
    display: none;
    padding: 5px;
    min-width: 50px;
    min-height: 25px;
    background-color: #541805;
    color: #fff;
    text-transform: capitalize;
    border-radius: 2px;
    border: none;
}
.sit-product-360-view span:before{
    content: '';
    background: url(../images/duble-arrow-gray.png) no-repeat;
    width: 13px;
    height: 10px;
    display: inline-block;
    margin: 5px;
    vertical-align: middle;
}
.sit-product-360-view span{
    cursor: pointer;
}
.sit-360-modal{
    width: 70%;
    max-width: 900px;
    margin: auto;
    top: 50%;
    transform: translateY(-50%);
    height: 73%;
}
.sit-360-popup{
    display: none;
}
/*End By MJ [For 360 view] [29.05.2019]*/

